{
    "name": "phariscope/doctrineenumtype",
    "type": "library",
    "description": "A way to create quickly and well tested doctrine type for your php enums",
    "keywords": ["Doctrine", "enum", "type"],
    "homepage": "https://github.com/phariscope/DoctrineEnumType",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Frédéric Royet",
            "email": "frederic.royet@logipro.com",
            "homepage": "https://www.linkedin.com/in/frederic-royet-42568566/",
            "role": "Project Founder"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "4.* || 3.*",
        "phariscope/safephp": "1.*"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.29",
        "phpstan/phpdoc-parser": "^1.20",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "Phariscope\\DoctrineEnumType\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Phariscope\\DoctrineEnumType\\Tests\\" : "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
